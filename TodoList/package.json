{
  "name": "list-filter",
  "version": "1.0.0",
  "description": "A simple filterable list",
  "author": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:enable-filter": "webpack --env.filter --config webpack.prod.js",
    "start": "webpack-dev-server --open --config webpack.dev.js",
