    "deploy": "surge --project ./dist"
  },
  "dependencies": {
    "melody-component": "^1.0.4",
    "melody-hoc": "^1.0.4",
    "melody-idom": "^1.0.4",
    "melody-parser": "^1.0.4",
    "melody-redux": "^1.0.4",
    "melody-traverse": "^1.0.4",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-polyfill": "^6.3.14",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.3.19",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "melody-loader": "^1.0.4",
    "melody-plugin-idom": "^1.0.4",
    "melody-types": "^1.0.4",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.13.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.9.3",
    "webpack-merge": "^4.1.2"
  }
